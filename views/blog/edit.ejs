<%include ../partials/header%>



<div class="container">
    <h1 class="text-center">Welcome to Edit Page</h1>
    <div class="thumbnail">
        <form action="/blogs/<%=blog._id%>?_method=PUT" method="POST">
            <div class="form-group">
                <span class="label label-default">Name</span>
                <input class="form-control" type="text" name="blog[name]" value="<%=blog.name%>"> 
            </div>
            <div class="form-group">
                <span class="label label-default">FamilyName</span>
                <input class="form-control" type="text" name="blog[familyname]" value="<%=blog.familyname%>"> 
            </div>
            <div class="form-group">
                <span class="label label-default">Profile-Image</span>
               <input class="form-control" type="text" name="blog[image]" value="<%=blog.image%>">
            </div>
            <button class="btn btn-primary btn-block">Submit</button>
        </form>
    </div>
</div>

<%include ../partials/footer%>


